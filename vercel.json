{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/dogapp/(.*)",
            "dest": "/dogapp/$1"
        },
        {
            "src": "/webshop/(.*)",
            "dest": "/webshop/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}

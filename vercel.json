{
    "version": 2,
    "builds": [
      {
        "src": "webshop/app.py",
        "use": "@vercel/python"
      },
      {
        "src": "webshop/static/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "dogapp/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/webshop/app.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/webshop/static/$1"
      },
      {
        "src": "/dogapp/(.*)",
        "dest": "/dogapp/$1"
      },
      {
        "src": "/webshop/(.*)",
        "dest": "/webshop/$1"
      },
      {
        "src": "/admin/(.*)",
        "dest": "/webshop/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  
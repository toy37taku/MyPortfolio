{
  "version": 2,
  "builds": [
    {
      "src": "webshop/app.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": [
          "webshop/static/**",
          "webshop/static/data/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/webshop/app.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/webshop/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/webshop/app.py"
    }
  ]
}
